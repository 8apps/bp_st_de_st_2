{
    "name": "Docs",
    "js": [
        {
            "path": "touch/sencha-touch-jsduck.js"
        },
        {
            "path": "touch-app.js",
            "bundle": true
        }
    ],

    "url": "touch-template.html",

    // "css": [
    //     {
    //         "path": "resources/css/app.css",
    //         "update": "delta"
    //     }
    // ],

    "appCache": {
        "cache": [
            "touch-template.html"
        ],
        "network": [
            "*"
        ],
        "fallback": []
    },

    "indexHtmlPath": "touch-template.html",

    /**
     * Extra resources to be copied along when build
     */
    "resources": [
    ],

    /**
     * Directory path to store all previous production builds. Note that the content generated inside this directory
     * must be kept intact for proper generation of delta between updates
     */
    "archivePath": "archive",

    /**
     * Default paths to build this application to for each environment
     */
    "buildPaths": {
        "production": "touch-build",
        "testing": "touch-testing",
        "package": "touch-package"
    },

    /**
     * Build options
     */
    "buildOptions": {
        "product": "touch",
        "minVersion": 3,
        "debug": false,
        "logger": "no"
    },

    /**
     * Uniquely generated id for this application, used as prefix for localStorage keys.
     * Normally you should never change this value.
     */
    "id": "905e61a0-71fb-11e1-b42a-61a0c6271820"
}
